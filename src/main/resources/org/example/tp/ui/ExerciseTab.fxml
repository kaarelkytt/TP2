                  <Button fx:id="upButton" disable="true" mnemonicParsing="false" onMouseClicked="#upButtonClicked" prefWidth="85.0" text="Move Up" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" />
