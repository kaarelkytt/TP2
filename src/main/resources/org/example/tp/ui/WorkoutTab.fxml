                  <ImageView fx:id="nextExerciseImage" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="55.0" AnchorPane.rightAnchor="25.0" />
               </children>
            </AnchorPane>
